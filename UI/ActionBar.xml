<?xml version="1.0" encoding="utf-8"?>
<Ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.blizzard.com/wow/ui/" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
    <Frame name="ActionBarOverlayTemplate" mixin="ActionBarOverlay" virtual="true">
        <Scripts>
    <OnLoad method="OnLoad" />
    </Scripts>
    <Anchors>
        <Anchor point="CENTER" />
    </Anchors>
    <Frames>
        <Cooldown parentKey="Cooldown" />
    </Frames>
    <Layers>
        <Layer level="ARTWORK">
        <Texture parentKey="Glow" setAllPoints="true" alphaMode="ADD" file="UI-HUD-ActionBar-Channel-InnerGlow" />
        </Layer>
        <Layer level="OVERLAY">
        <FontString parentKey="countStack" inherits="NumberFontNormal">
            <Anchors>
            <Anchor point="TOPLEFT" x="5" y="-5" />
            </Anchors>
        </FontString>
        </Layer>
    </Layers>
    </Frame>
</Ui>
